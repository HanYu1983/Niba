<tableview width="100%" height="100%" contentWidth="100%" style="font-size:12;">
    <header width="100%">
        <column id="name" text="名稱" width="100%" sortable="false" style="text-align:center;font-size:12;"/>
        <column id="score" text="分數" width="80" sortable="true" style="text-align:center;font-size:12;"/>
        <column id="money" text="金錢" width="150" sortable="true" style="text-align:center;font-size:12;"/>
        <column id="food" text="糧草" width="150" sortable="true" style="text-align:center;font-size:12;"/>
        <column id="army" text="士兵" width="150" sortable="true" style="text-align:center;font-size:12;"/>
        <column id="peopleCount" text="武將數" width="60" sortable="true" style="text-align:center;font-size:12;"/>
        <column id="cityCount" text="城池數" width="60" sortable="true" style="text-align:center;font-size:12;"/>
        <column id="treasureCount" text="寶物數" width="60" sortable="true" style="text-align:center;font-size:12;"/>
    </header>

    <item-renderer verticalAlign="center">
        <label id="name" textAlign="center" width="100%" />
    </item-renderer>

    <data>

    </data>
</tableview>